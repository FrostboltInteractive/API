{
  "functions": {
    "api/control/*": {
      "runtime": "python3.9"
    },
    "api/proxy/*": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/control/(.*)", "destination": "/api/control/API.py" },
    { "source": "/proxy/(.*)", "destination": "/api/proxy/server.js" }
  ]
}